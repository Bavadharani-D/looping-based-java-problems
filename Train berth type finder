import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      int T = sc.nextInt();
      T= T%8;
      if(T == 1 || T==4)
        System.out.println("LB");
      else if (T== 2 || T == 5)
        System.out.println("MB");
      else if (T == 3 || T == 6)
        System.out.println("UB");
      else if(T == 7)
        System.out.println("SL");
      else if (T == 0)
        System.out.println("SU");
  }
}
