import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
      float f =sc.nextFloat();
      int a = sc.nextInt();
      int b = sc.nextInt();
      int c = sc.nextInt();
      System.out.println((int)Math.floor(f));
      System.out.println((int)Math.ceil(f));
      System.out.println((int)Math.round(f));
      System.out.println((int)Math.sqrt(a));
      System.out.println((int)Math.pow(b,c));
      }
}
